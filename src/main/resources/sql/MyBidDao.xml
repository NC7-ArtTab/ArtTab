<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="arttab.server.dao.MyBidDao">

    <!-- findByMemNo 메서드의 SQL 쿼리 정의 -->
    <select id="findByMemNo" parameterType="int" resultType="arttab.server.vo.MyBid">
        SELECT b.bid_no, b.art_no, b.mem_no, b.bid_price, b.bid_datetime
        FROM bid b
        WHERE b.mem_no = #{memNo}
    </select>

</mapper>
