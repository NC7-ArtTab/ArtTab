<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="arttab.server.dao.MemberDao">
    <update id="updateMember" parameterType="arttab.server.vo.Member">
        UPDATE member
        SET
        mem_name = #{mem_name},
        mem_phone = #{mem_phone},
        mem_addr = #{mem_addr},
        mem_detail_addr = #{mem_detail_addr},
        mem_acc = #{mem_acc}
        WHERE mem_no = #{mem_no}
    </update>

    <select id="findBy" parameterType="int" resultType="arttab.server.vo.Member">
        SELECT *
        FROM member
        WHERE mem_no = #{memNo}
    </select>
</mapper>
